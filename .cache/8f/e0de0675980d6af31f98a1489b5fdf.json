4b;\r\n$darkish-blue: #120d25;\r\n$matte-black: #060608;\r\n$metallic-grey: #1F2833;\r\n$aqua: #55FCF1;\r\n$box-shadow-blue1: rgba(31 ,38, 135, 0.27);\r\n$box-shadow-blue2: rgba(31 ,38, 135, 0.67);\r\n$cream-white: #dfd2b8; \r\n\r\n\r\n\r\n// PRDEFINED\r\n\r\n.section {\r\n    padding: 1rem;\r\n    overflow: hidden;\r\n    min-height: 100vh;\r\n    padding-top: 5rem;\r\n    @media (min-width: 650px) {\r\n        padding: 5rem;\r\n    }\r\n    &__header {\r\n        &-title {\r\n            font-weight: 400;\r\n            font-size: 17px;\r\n            color: #666;\r\n        }\r\n        &-subtitle {\r\n            font-size: 27px;\r\n            font-weight: 700;\r\n        }\r\n    }\r\n}","assets\\style\\services.scss":"@import \"./base.scss\";\r\n@import url('https://fonts.googleapis.com/css2?family=Roboto:wght@300&family=Syne&display=swap');\r\n@import url('http://fonts.cdnfonts.com/css/helvetica-neue-9');\r\n@import url('http://fonts.cdnfonts.com/css/futura-md-bt');\r\n@import url('http://fonts.cdnfonts.com/css/avenir-next-lt-pro');\r\n\r\n\r\n.services {\r\n    background: $matte-black;\r\n    padding: 50px;\r\n    color: #dfd2b8; \r\n    position: relative;\r\n    width: 100%;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    gap: 50px;\r\n    flex-wrap: wrap;\r\n    \r\n    &__box {\r\n        position: relative;\r\n        width: 20rem;\r\n        height: 280px;\r\n        background: #080715;\r\n        border-radius: 20px;\r\n        overflow: hidden;\r\n        .icon {\r\n            position: absolute;\r\n            top: 0;\r\n            left: 0;\r\n            width: 100%;\r\n            height: 100%;\r\n            transition: 0.5s;\r\n            display: flex;\r\n            justify-content: center;\r\n            align-items: center;\r\n            z-index: 1;\r\n            img {\r\n                filter: invert(100%);\r\n                width: 35%;\r\n            }\r\n\r\n           \r\n            transition-delay: 0.25s;\r\n           \r\n\r\n        } \r\n        .red {\r\n            background: linear-gradient(to right, #76C893, #65ac7e );\r\n        }\r\n\r\n        .blue {\r\n            background: linear-gradient(to right, #34A0A4, #2c878a );\r\n        }\r\n\r\n        .purple {\r\n            background: linear-gradient(to right, #168AAD, #147694);\r\n        }\r\n        \r\n        &:hover .icon {\r\n            top: 20px;\r\n            left: calc(50% - 40px);\r\n            width: 80px;\r\n            height: 80px;\r\n            border-radius: 50%;\r\n        }\r\n\r\n        \r\n        .content {\r\n            position: relative;\r\n            padding: 10px;\r\n            color: $cream-white;\r\n            text-align: center;\r\n            margin-top: 50px;\r\n            z-index: 1;\r\n            transform: scale(0);\r\n            transition: 0.5s;\r\n            transition-delay: 0s;\r\n        }\r\n\r\n        &:hover .content {\r\n            transform: scale(1);\r\n            transition-delay: 0.25s;\r\n            \r\n        }\r\n\r\n        .content h2 {\r\n            font-size: 24px;\r\n            transition-delay: 0.25s;\r\n        }\r\n\r\n        .content p {\r\n            line-height: 1.5rem;\r\n            font-size: 1rem;\r\n            padding-bottom: 15rem;\r\n            transition-delay: 0.25s;\r\n        }\r\n    }\r\n} \r\n\r\n\r\n        "},"lineCount":null}},"error":null,"hash":"656259956343304610d0836ebedfc21d","cacheData":{"env":{}}}